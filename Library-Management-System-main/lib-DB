create database libma;
 use libma;
 create table studen(
 studentid varchar(10) primary key,
 namestd varchar(100), 
 course varchar(10),
 streams varchar(10)
 );
 
create table abook(
 codee varchar(10) primary key,
 bname varchar(100), 
 quantity varchar(10)
 );
 
 create table iso(
 bookname varchar(100),
 studentname varchar(100), 
 isse varchar(10),
 due varchar(10),
 returnBook varchar(30)
 );
